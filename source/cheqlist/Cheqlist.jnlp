<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="6.0+" codebase="http://eriwen.com/cheqlist/" href="Cheqlist.jnlp">
    <information>
        <title>Cheqlist</title>
        <vendor>Eric Wendelin</vendor>
        <homepage href="http://github.com/eriwen/cheqlist-javafx"/>
        <description>A to-do list manager powered by Remember The Milk</description>
        <icon href="cheqlist_logo_256.png" width="256" height="256"/>
        <icon kind="shortcut" href="cheqlist_logo_16.png" width="16" height="16"/>
        <icon kind="shortcut" href="cheqlist_logo_32.png" width="32" height="32"/>
        <icon kind="shortcut" href="cheqlist_logo_64.png" width="64" height="64"/>
        <icon kind="splash" href="cheqlist_logo_256.png" width="256" height="256"/>
        <shortcut>
            <desktop/>
            <menu submenu="Cheqlist"/>
        </shortcut>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.6+"/>
        <property name="jnlp.packEnabled" value="false"/>
        <property name="jnlp.versionEnabled" value="true"/>
        <extension name="JavaFX Runtime" href="http://dl.javafx.com/1.3/javafx-rt.jnlp"/>
        <jar href="Cheqlist.jar" main="true"/>
        <jar href="lib/miglayout-3.7.jar"/>
        <jar href="lib/JFXtras-Common-0.7rc2.1.jar"/>
        <jar href="lib/JFXtras-Controls-0.7rc2.jar"/>
        <jar href="lib/JFXtras-Test-0.7rc1.jar"/>
        <jar href="lib/groovy-all.jar"/>
        <jar href="lib/groovyrtm.jar"/>
        <jar href="lib/swing-worker-1.2.jar"/>
        <jar href="lib/commons-codec-1.3.jar"/>
    </resources>
    <application-desc main-class="com.sun.javafx.runtime.main.Main">
        <argument>MainJavaFXScript=org.eriwen.cheqlist.Main</argument>
    </application-desc>
    <update check="always" policy="prompt-update"/>
</jnlp>
